      <bpmn:lane id="Lane_Revisor" name="Revisor" />
      <bpmn:lane id="Lane_Servicios" name="Servicios Externos" />
      <bpmn:lane id="Lane_Autor" name="Autor">
        <bpmn:flowNodeRef>Task_Edicion</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_EnviarRevision</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_AsignarRevisores</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Revision</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_AprobacionEditorial</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Publicar</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Notificar</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_ActualizarContenido</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>StartEvent_Publicacion</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Event_PublicationSubmitted</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Event_PublicationPublished</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_Decision</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>EndEvent_Rechazado</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>EndEvent_Publicado</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Event_ReviewRequested</bpmn:flowNodeRef>
      </bpmn:lane>
    </bpmn:laneSet>
    <bpmn:startEvent id="StartEvent_Publicacion" name="Inicio: Crear borrador">
      <bpmn:outgoing>Flow_1</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:userTask id="Task_Edicion" name="Añadir contenido, metadatos y adjuntos">
      <bpmn:incoming>Flow_1</bpmn:incoming>
      <bpmn:outgoing>Flow_2</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:userTask id="Task_EnviarRevision" name="Enviar a revisión">
      <bpmn:incoming>Flow_2</bpmn:incoming>
      <bpmn:outgoing>Flow_3</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:intermediateThrowEvent id="Event_PublicationSubmitted" name="publication.submitted">
      <bpmn:incoming>Flow_3</bpmn:incoming>
      <bpmn:outgoing>Flow_4</bpmn:outgoing>
      <bpmn:messageEventDefinition />
    </bpmn:intermediateThrowEvent>
    <bpmn:userTask id="Task_AsignarRevisores" name="Asignar revisores">
      <bpmn:incoming>Flow_4</bpmn:incoming>
      <bpmn:outgoing>Flow_5</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:intermediateThrowEvent id="Event_ReviewRequested" name="publication.review.requested">
      <bpmn:incoming>Flow_5</bpmn:incoming>
      <bpmn:outgoing>Flow_6</bpmn:outgoing>
      <bpmn:messageEventDefinition />
    </bpmn:intermediateThrowEvent>
    <bpmn:userTask id="Task_Revision" name="Revisión y comentarios">
      <bpmn:incoming>Flow_6</bpmn:incoming>
      <bpmn:outgoing>Flow_7</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:exclusiveGateway id="Gateway_Decision" name="Decisión del revisor">
      <bpmn:incoming>Flow_7</bpmn:incoming>
      <bpmn:outgoing>Flow_Aceptar</bpmn:outgoing>
      <bpmn:outgoing>Flow_SolicitarCambios</bpmn:outgoing>
      <bpmn:outgoing>Flow_Rechazar</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:userTask id="Task_AprobacionEditorial" name="Aprobación editorial">
      <bpmn:incoming>Flow_Aceptar</bpmn:incoming>
      <bpmn:outgoing>Flow_8</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:userTask id="Task_ActualizarContenido" name="Actualizar contenido y reenviar">
      <bpmn:incoming>Flow_SolicitarCambios</bpmn:incoming>
      <bpmn:outgoing>Flow_BackRevision</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:endEvent id="EndEvent_Rechazado" name="Publicación rechazada">
      <bpmn:incoming>Flow_Rechazar</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:serviceTask id="Task_Publicar" name="Publicar (servicio)" camunda:type="external">
      <bpmn:incoming>Flow_8</bpmn:incoming>
      <bpmn:outgoing>Flow_9</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:intermediateThrowEvent id="Event_PublicationPublished" name="publication.published">
      <bpmn:incoming>Flow_9</bpmn:incoming>
      <bpmn:outgoing>Flow_10</bpmn:outgoing>
      <bpmn:messageEventDefinition />
    </bpmn:intermediateThrowEvent>
    <bpmn:serviceTask id="Task_Notificar" name="Enviar notificaciones (servicio)" camunda:type="external">
      <bpmn:incoming>Flow_10</bpmn:incoming>
      <bpmn:outgoing>Flow_11</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:endEvent id="EndEvent_Publicado" name="Publicación visible en catálogo">
      <bpmn:incoming>Flow_11</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_1" sourceRef="StartEvent_Publicacion" targetRef="Task_Edicion" />
    <bpmn:sequenceFlow id="Flow_2" sourceRef="Task_Edicion" targetRef="Task_EnviarRevision" />
    <bpmn:sequenceFlow id="Flow_3" sourceRef="Task_EnviarRevision" targetRef="Event_PublicationSubmitted" />
    <bpmn:sequenceFlow id="Flow_4" sourceRef="Event_PublicationSubmitted" targetRef="Task_AsignarRevisores" />
    <bpmn:sequenceFlow id="Flow_5" sourceRef="Task_AsignarRevisores" targetRef="Event_ReviewRequested" />
    <bpmn:sequenceFlow id="Flow_6" sourceRef="Event_ReviewRequested" targetRef="Task_Revision" />
    <bpmn:sequenceFlow id="Flow_7" sourceRef="Task_Revision" targetRef="Gateway_Decision" />
    <bpmn:sequenceFlow id="Flow_Aceptar" sourceRef="Gateway_Decision" targetRef="Task_AprobacionEditorial" />
    <bpmn:sequenceFlow id="Flow_SolicitarCambios" sourceRef="Gateway_Decision" targetRef="Task_ActualizarContenido" />
    <bpmn:sequenceFlow id="Flow_Rechazar" sourceRef="Gateway_Decision" targetRef="EndEvent_Rechazado" />
    <bpmn:sequenceFlow id="Flow_8" sourceRef="Task_AprobacionEditorial" targetRef="Task_Publicar" />
    <bpmn:sequenceFlow id="Flow_9" sourceRef="Task_Publicar" targetRef="Event_PublicationPublished" />
    <bpmn:sequenceFlow id="Flow_10" sourceRef="Event_PublicationPublished" targetRef="Task_Notificar" />
    <bpmn:sequenceFlow id="Flow_11" sourceRef="Task_Notificar" targetRef="EndEvent_Publicado" />
    <bpmn:sequenceFlow id="Flow_BackRevision" sourceRef="Task_ActualizarContenido" targetRef="Task_Revision" />
  </bpmn:process>
